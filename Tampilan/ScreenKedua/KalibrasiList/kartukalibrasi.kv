<KartuKalibrasi>:
    md_bg_color: '#F4F6FC'
    line_color: '#DEDAD1'
    line_width: 5
    padding: '20dp'
    IsiKalibrasi:


<Isikalibrasi>:
    orientation: 'vertical'
    spacing: 3
    size_hint: 1, 1
    NormalKalibrasi:
        size_hint: 1, 1
    PekingKalibrasi:
        size_hint: 1, 1
    KenongKalibrasi:
        size_hint: 1, 1
    GongKalibrasi:
        size_hint: 1, 1

<NormalKalibrasi>:
    md_bg_color: '#D9D9D9'
    MDRelativeLayout:
        MDLabel:
            pos_hint: {'x': 27/582, 'top': (112-10)/112}
            adaptive_size: True
            text: 'Bilah Normal :'

        TombolNormal:
            pos_hint: {'x': 27/582, 'top': (112-42)/112}
            size_hint: 527/582, 58/112
            MDScrollView:
                do_scroll_x: True
                do_scroll_y: False
                MDBoxLayout:
                    adaptive_width: True
                    MDRectangleFlatButton:
                        size_hint: None, 1
                        text: 'Bilah 1'
                        theme_color_text: 'Custom'
                        text_color: 'black'
                        line_color: '#7E544E'
                        line_width: 2
                        valign: 'center'
                        halign: 'center'
                        on_press: root.bilah1()
                        md_bg_color: app.kondisitombol.bcn1

                    MDRectangleFlatButton:
                        size_hint: None, 1
                        text: 'Bilah 2'
